import java.util.Scanner;

public class Checkpoint3 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String statement = input.nextLine();
		System.out.println(produceAnswer(statement));
	}
	
	
	public static String produceAnswer(String str){
		String numerator1 = "0";
		String numerator2 = "0";
		String denominator1 = "1";
		String denominator2 = "1";
		String whole1 = "0";
		String whole2 = "0";
		String op1 = str.substring(0, str.indexOf(" "));
		String op2 = str;
		
		
		if(str.indexOf("+")>0){
			op2 = str.substring(str.indexOf(" + ")+3);
		}
		else if(str.indexOf(" - ")>0){
			op2 = str.substring(str.indexOf(" - ")+3);
		}
		else if(str.indexOf("*")>0){
			op2 = str.substring(str.indexOf(" * ")+3);
		}
		else if(str.indexOf(" / ")>0){
			op2 = str.substring(str.indexOf(" / ")+3);
		}
		
		if (op1.indexOf("_")>0){
			whole1 = op1.substring(0, op1.indexOf("_"));
			if(op1.indexOf("/")>0){
				numerator1 = op1.substring(op1.indexOf("_")+1, op1.indexOf("/"));
				denominator1 = op1.substring(op1.indexOf("/")+1);
			}
			else{
				numerator1 = "0";
				denominator1 = "1";
				whole1 = op1.substring(0);
			}
		}
		else{
			if(op1.indexOf("/")>0){
				numerator1 = op1.substring(0, op1.indexOf("/"));
				denominator1 = op1.substring(op1.indexOf("/")+1);
			}
			else{
				whole1 = op1;
			}
		}
		
		
		if (op2.indexOf("_")>0){
			whole2 = op2.substring(0, str.indexOf("_"));
			if(op2.indexOf("/")>0){
				numerator2 = op2.substring(op2.indexOf("_")+1, op2.indexOf("/"));
				denominator2 = op2.substring(op2.indexOf("/")+1);
			}
			else{
				numerator2 = "0";
				denominator2 = "1";
				whole2 = op2.substring(0);
			}
		}
		else{
			if(op2.indexOf("/")>0){
				numerator2 = op2.substring(0, op2.indexOf("/"));
				denominator2 = op2.substring(op2.indexOf("/")+1);
			}
			else{
				whole2 = op2;
			}
		}
		
		return ("whole number:"+whole+" numerator:"+numerator+" denominator:"+denominator);
	}
}
