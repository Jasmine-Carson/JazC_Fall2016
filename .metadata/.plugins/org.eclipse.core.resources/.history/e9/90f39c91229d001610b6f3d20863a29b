
public class LotsOfCopies {

	public static void main(String[] args) {
		int num = 7;
		String strMain = "APCS";
		int[] arrMain = {1, 2, 3, 4, 5};
		changeMe(num, strMain, arrMain);
	}
	public static void changeMe(int x, String str, int[] arr){
		x++;
		str = str.substring(0,str.length()-2);
		arr[1]=8;
	}
}